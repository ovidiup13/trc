{
	"name": "@trc/e2e",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"test": "vitest run --passWithNoTests --exclude src/*e2e.test.ts",
		"test:e2e": "vitest run --passWithNoTests src/*.e2e.test.ts",
		"test:docker": "bun run test:e2e",
		"e2e": "bun run test:e2e",
		"e2e:prep": "sh ./scripts/e2e-prep.sh",
		"build:runner": "sh ./scripts/build-runner.sh",
		"build:server": "sh ./scripts/build-server.sh"
	},
	"dependencies": {
		"jose": "^5.6.0"
	}
}
