{
	"name": "@trc/e2e",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"test": "vitest run --passWithNoTests --exclude src/*e2e.test.ts",
		"test:e2e": "vitest run --passWithNoTests src/*.e2e.test.ts",
		"test:docker": "bun run test:e2e",
		"e2e": "bun run test:e2e",
		"e2e:prep": "bun run build:runner && bun run build:server",
		"build:runner": "docker build -f docker/Dockerfile.runner -t trc-e2e-runner:latest ../..",
		"build:server": "docker build -f docker/Dockerfile.server -t trc-e2e-server:latest ../.."
	},
	"dependencies": {
		"jose": "^5.6.0"
	}
}
